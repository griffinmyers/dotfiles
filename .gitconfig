[alias]
  s = status
  c = commit
  a = add
  l = log
[user]
	name = Will Myers
	email = griffin.myers@gmail.com
[push]
	default = simple
[core]
    excludesfile = /Users/wmyers1/.gitignore
[credential]
    helper = osxkeychain
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
