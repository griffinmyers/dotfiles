[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[user]
    email = wmyers1@bloomberg.net
    name = Will Myers
[push]
    default = current # `simple` if > git 2.0
[core]
    excludesfile = /Users/wmyers1/.gitignore
[alias]
    s = status
    c = commit
    a = add
    l = log
[credential]
    helper = osxkeychain
