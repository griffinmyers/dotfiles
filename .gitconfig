[alias]
  s = status
  b = branch
  c = commit -v
  co = checkout
  a = add
  aa = add -A .
  l = log
  ls = log --pretty=format:'%C(yellow)%h%Cred%d %Creset%s%Cblue [%cn]' --decorate
[user]
  name = Will Myers
  email = will@usebutton.com
[push]
  default = current
[core]
  excludesfile = /Users/williammyers/.gitignore
[credential]
  helper = osxkeychain
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
